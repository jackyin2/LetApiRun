{
   "test": [{
     "name": "测试登录功能正常",

     "setupcase": {
       "success": "${__get_value('success')}",
       "abc":123,
       "c":3,
       "fun2":"${__get_value(a=1, b=2)}"
     },

     "requestor":{
        "url": "${host}/device-mgr/#/login/${abc}/${fun2}",
        "method": "POST",
        "headers": {
          "content-type": "application/json",
          "token": "${token}"
       },
     "data": { "name": "user1", "password": "${success}", "token":"${abc}"}
     },

     "validator": {
       "code": 200,
       "headers": {"Content-Type": "application/json;charset=UTF-8"},
       "assertEqStr": ["user created successfully", "true", "100000"],
       "assertEqJson":{"success":true, "msg":"abc"}
     },

     "collector":{
       "json":{"token": "${get_value('token')}"},
       "xml":{"token": "${get_value('token')}"},
       "html":{"token": "${get_value('token')}"}
     },

     "teardowncase":{
       "clearcookies": "clearcookies",
       "clearsession": "clearsession",
       "cleartoken": "cleartoken"
     }
   }]
}